{
  "name": "openvectoreditor",
  "displayName": "openvectoreditor",
  "description": "DNA editor in vscode",
  "version": "1.0.0",
  "publisher": "sanekun",
  "repository": {
    "type": "git",
    "url": "https://github.com/sanekun/ove-vscode"
  },
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:openvectoreditor.show",
    "onCustomEditor:openvectoreditor.editor"
  ],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "dna",
        "extensions": [
          ".dna",
          ".fasta",
          ".gb"
        ],
        "aliases": [
          "DNA",
          "dna"
        ]
      }
    ],
    "customEditors": [
      {
        "viewType": "openvectoreditor.editor",
        "displayName": "OVE",
        "selector": [
          {
            "filenamePattern": "*.dna"
          },
          {
            "filenamePattern": "*.fasta"
          },
          {
            "filenamePattern": "*.gb"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "openvectoreditor.show",
        "title": "openvectoreditor:OVE"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.98.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.21.0",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "@teselagen/bio-parsers": "^0.4.27",
    "@teselagen/ove": "^0.7.27",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
